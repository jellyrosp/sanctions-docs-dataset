    "import matplotlib.pyplot as plt\n",
    "import pandas as pd\n",
    "\n",
    "# Load Tabella_4 (assumes columns: Designation_source, AB, PB, SB, FMS)\n",
    "tabella_4 = pd.read_csv(\"Tabella_4.csv\")\n",
    "\n",
    "# Remove the 'Totale' row\n",
    "tabella_4 = tabella_4[tabella_4[\"Designation_source\"] != \"**Totale**\"]\n",
    "\n",
    "# Filter out rows where all counts are 0\n",
    "tabella_4 = tabella_4[\n",
    "    (tabella_4[[\"AB\", \"PB\", \"SB\", \"FMS\"]].sum(axis=1)) > 0\n",
    "]\n",
    "\n",
    "# Extract labels\n",
    "designation_sources = tabella_4[\"Designation_source\"].tolist()\n",
    "counts = tabella_4[[\"AB\", \"PB\", \"SB\", \"FMS\"]].astype(int).values.tolist()\n",
    "\n",
    "# Pie chart labels and colors\n",
    "labels = ['AB', 'PB', 'SB', 'FMS']\n",
    "colors = ['#ff9999', '#66b3ff', '#99ff99', '#ffcc99']\n",
    "\n",
    "# Plot setup\n",
    "n = len(designation_sources)\n",
    "fig, axes = plt.subplots(1, n, figsize=(4*n, 4))\n",
    "if n == 1:\n",
    "    axes = [axes]\n",
    "\n",
    "# Draw each pie chart\n",
    "for i, ax in enumerate(axes):\n",
    "    ax.pie(\n",
    "        counts[i], \n",
    "        labels=None, \n",
    "        autopct='%1.1f%%', \n",
    "        startangle=90, \n",
    "        colors=colors\n",
    "    )\n",
    "    ax.set_title(designation_sources[i], fontsize=10)\n",
    "\n",
    "# Unified legend\n",
    "fig.legend(labels, loc='lower center', ncol=4, bbox_to_anchor=(0.5, -0.05))\n",
    "\n",
    "# Adjust layout\n",
    "plt.tight_layout()\n",
    "plt.show()\n"
   ]
