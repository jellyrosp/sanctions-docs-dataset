    "designation_source_list = tabella_2['Designation_source'].tolist()[:-1]\n",
    "genere_indicato_totals = tabella_2['Genere indicato'].tolist()[:-1]\n",
    "\n",
    "# Individui femminili\n",
    "female_df = final_table_df[final_table_df['Gender'] == 'female']\n",
    "femmine = (\n",
